{
  "name": "todo-list-typescript",
  "version": "1.0.0",
  "description": "Sistema de gerenciamento de tarefas em TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
